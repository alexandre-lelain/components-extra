{
  "name": "components-extra",
  "version": "0.2.2",
  "description": "React UI components based on styled-components & material-ui.",
  "repository": "git@github.com:alexandre-lelain/components-extra.git",
  "homepage": "https://components-extra.netlify.com/",
  "author": "Alexandre Le Lain <lelain.alexandre@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "components",
    "material-ui",
    "simple"
  ],
  "main": "build/bundle.js",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "yarn webpack",
    "build:analyze": "yarn build --analyze",
    "build-storybook": "build-storybook -c .storybook",
    "lint": "eslint --cache {src,stories}/**/*.js",
    "prepare": "yarn build",
    "prettier": "prettier --write {src,stories}/**/*.js",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "postversion": "git push && git push --tags",
    "start": "yarn start-storybook -p 4242",
    "test": "jest",
    "version": "yarn prettier && git add -vA src"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/react": "^5.1.10",
    "@storybook/theming": "^5.1.11",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.2",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "husky": "^3.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-json-view": "^1.19.1",
    "react-markdown": "^4.2.2",
    "style-loader": "^1.0.0",
    "styled-components": "^4.3.2",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"{.storybook,src,stories}/**/*.js\""
    }
  },
  "jest": {
    "setupFilesAfterEnv": ["./jest.setup.js"],
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^theme(.*)$": "<rootDir>/src/theme$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^test(.*)$": "<rootDir>/src/test$1"
    }
  }
}
