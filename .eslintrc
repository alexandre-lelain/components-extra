{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/prop-types": [
      2, { "ignore": ["theme", "children"] }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
